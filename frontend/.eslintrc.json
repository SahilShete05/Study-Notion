{
  "extends": ["react-app", "plugin:import/errors", "plugin:import/warnings"],
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": ["error", { "caseSensitive": true }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
